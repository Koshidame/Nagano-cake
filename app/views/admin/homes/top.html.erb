<table class='table table-hover table-inverse'>
  <h1>注文一覧（TOPページ）</h1>
  <thead>
    <tr>
      <th>購入日時</th>
      <th>購入者</th>
      <th>注文個数</th>
      <th>注文ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= link_to admin_order_detail do %>
          <%= order.created_at %>
          <% end %>
        </td>
        <td>#{"order.last_name""order.first_name"}</td>
        <td><%= order.amount %></td>
        <td><%= order.making_status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<!--10件ずつ表示を分ける必要あり-->